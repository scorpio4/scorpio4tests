@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix asq: 		<http://scorpio4.com/vocab/v1/asq/>.
@prefix : 			<http://scorpio4.com/vocab/v1/asq/>.

#
# Abstract Query Notation
#

:	a					owl:Ontology;
	owl:versionInfo		"0.1";
	rdfs:label			"ASQ";
	dc:title			"ASQ Queries";
	rdfs:comment		"Vocabulary for describing queries.";
	dc:created			"2013-07-01"^^xsd:date;
	dc:modified			"2013-07-01"^^xsd:date;
	dc:license 			<http://www.opendatacommons.org/licenses/pddl/1.0/>
.

:Query
	a					rdfs:Class;
	rdfs:label			"Query";
	rdfs:comment 		"A Query constrained of Patterns";
	rdfs:isDefinedBy	:;
.

:Inference
	a					rdfs:Class;
	rdfs:label			"Inference";
	rdfs:comment 		"An Inference Query produces of Patterns";
	rdfs:isDefinedBy	:;
.

:Intuition
	a					rdfs:Class;
	rdfs:label			"Intuition";
	rdfs:comment 		"An Inference based on a Query";
	rdfs:isDefinedBy	:;
.

:Pattern
	a					rdfs:Class;
	rdfs:label			"Pattern";
	rdfs:comment 		"A Pattern consists of ?that ?has ?this clauses";
	rdfs:isDefinedBy	:;
.

:Term
	a					rdfs:Class;
	rdfs:label			"Term";
	rdfs:comment 		"A clause that can be matched";
	rdfs:isDefinedBy	:;
.

:where
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"where";
	rdfs:domain			:Query;
	rdfs:range  		:Pattern;
.

:nested
	rdfs:subPropertyOf	:where;
	rdfs:isDefinedBy	:;
	rdfs:label 			"next pattern";
#	rdfs:domain			:Pattern;
	rdfs:range  		:Pattern;
.
:that
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"?that";
	rdfs:range  		:Term;
	rdfs:domain			:Pattern;
.
:has
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"?has";
	rdfs:range  		:Term;
	rdfs:domain			:Pattern;
.
:this
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"?this";
	rdfs:range  		:Term;
	rdfs:domain			:Pattern;
.
:optional
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"optional";
	rdfs:domain			:Query;
	rdfs:range  		:Pattern;
.

:learn
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"?that";
	rdfs:range  		:Inference;
	rdfs:domain			:Intuition;
.
:when
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"?that";
	rdfs:range  		:Query;
	rdfs:domain			:Intuition;
.
