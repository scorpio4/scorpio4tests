.
:containsString
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"contains"@en;
	rdfs:comment 		"test string matching"@en;
.
:endsWith
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"ends with"@en;
	rdfs:comment 		"test string matching"@en;
.
:startsWith
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
