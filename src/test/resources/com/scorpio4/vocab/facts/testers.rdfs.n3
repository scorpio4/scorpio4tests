.
:greaterThanOrEqualTo
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			">="@en;
	rdfs:comment 		"test ordering"@en;
.
:lessThan
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"<"@en;
	rdfs:comment 		"test ordering"@en;
.
:lessThanOrEqualTo
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"<="@en;
	rdfs:comment 		"test ordering"@en;
.
:equalToIgnoringCase
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"ignore case"@en;
	rdfs:comment 		"test string equality ignoring case"@en;
.
:equalToIgnoringWhiteSpace
	rdfs:subPropertyOf	:test;
	rdfs:isDefinedBy	:;
	rdfs:label 			"ignore spaces"@en;
