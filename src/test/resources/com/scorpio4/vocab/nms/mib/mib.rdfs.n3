@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix iana:       <http://www.iana.org/assignments/port-numbers/#>.
@prefix osi:        <http://skms.info/ontology/osi/model/>.
@prefix oui:        <http://standards.ieee.org/develop/regauth/oui/oui#>.
@prefix mib:        <http://www.iana.org/assignments/mib#>.

@prefix : 			<http://www.iana.org/assignments/mib#>.

#
# SNMP MIB Ontology
#

:	a					owl:Ontology;
	owl:versionInfo		"0.1";
	rdfs:label			"SNMP MIBs";
	dc:title			"SNMP MIBs";
	rdfs:comment		"Vocabulary for SNMP MIBs";
	dc:created			"2013-07-1"^^xsd:date;
	dc:modified			"2013-07-1"^^xsd:date;
	dc:license 			<http://www.opendatacommons.org/licenses/pddl/1.0/>
.

:MIB
	a					rdfs:Class;
	rdfs:label			"MIB";
	rdfs:comment 		"An SNMP Management Information Base";
	rdfs:isDefinedBy	:;
.

:Value
	a					rdfs:Class;
	rdfs:label			"Value";
	rdfs:comment 		"A Generic Value";
	rdfs:isDefinedBy	:;
.

:Scalar
    rdfs:subClassOf     :Value;
	rdfs:label			"Scalar";
	rdfs:comment 		"A Scalar Value";
	rdfs:isDefinedBy	:;
.

:Table
    rdfs:subClassOf     :Value;
	rdfs:label			"Table";
	rdfs:comment 		"A Table";
	rdfs:isDefinedBy	:;
.

:TableColumn
    rdfs:subClassOf     :Value;
	rdfs:label			"Table Column";
	rdfs:comment 		"A Table Column";
	rdfs:isDefinedBy	:;
.

:Sequence
    rdfs:subClassOf     :Value;
	rdfs:label			"Sequence";
	rdfs:comment 		"A Sequence";
	rdfs:isDefinedBy	:;
.


:contains
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"contains";
.

:access
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"network #";
	rdfs:range  		:Access;
.

:status
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"status";
	rdfs:range  		:Status;
.

:syntax
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"syntax";
	rdfs:range  		:Syntax;
.
