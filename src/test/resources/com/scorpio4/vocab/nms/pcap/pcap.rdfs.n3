@prefix rdf:		<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:		<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:		<http://www.w3.org/2002/07/owl#> .
@prefix dc:			<http://purl.org/dc/elements/1.1/> .
@prefix eg:			<http://www.example.org/> .
@prefix xsd: 		<http://www.w3.org/2001/XMLSchema#>.

@prefix iana:       <http://www.iana.org/assignments/port-numbers/#>.
@prefix osi:        <http://skms.info/ontology/osi/model/>.
@prefix oui:        <http://standards.ieee.org/develop/regauth/oui/oui#>.

@prefix pcap:		<urn:com.scorpio4:nms:pcap:>.
@prefix : 			<urn:com.scorpio4:nms:pcap:>.

#
# Network Sniffer
#

:	a					owl:Ontology;
	owl:versionInfo		"0.1";
	rdfs:label			"Network Sniffer";
	dc:title			"Network Sniffer";
	rdfs:comment		"Vocabulary for describing NMS packet capture.";
	dc:created			"2013-07-1"^^xsd:date;
	dc:modified			"2013-07-1"^^xsd:date;
	dc:license 			<http://www.opendatacommons.org/licenses/pddl/1.0/>
.

:Session
	a					rdfs:Class;
	rdfs:label			"Capture Session";
	rdfs:comment 		"A Capture Session";
	rdfs:isDefinedBy	:;
.

:Packet
	a					rdfs:Class;
	rdfs:label			"Packet";
	rdfs:comment 		"A Packet";
	rdfs:isDefinedBy	:;
.

:Host
	a					rdfs:Class;
	rdfs:label			"Host";
	rdfs:comment 		"An IP Network Host (IPv4, IPv6)";
	rdfs:isDefinedBy	:;
.

:Network
	a					rdfs:Class;
	rdfs:label			"Network";
	rdfs:comment 		"An IEEE-registered Computer Network";
	rdfs:isDefinedBy	:;
.

:Source
	a					rdfs:Class;
	rdfs:subClassOf     :Host;
	rdfs:label			"Packet Source";
	rdfs:comment 		"A Packet Source";
	rdfs:isDefinedBy	:;
.

:Destination
	a					rdfs:Class;
	rdfs:subClassOf     :Host;
	rdfs:label			"Packet Destination";
	rdfs:comment 		"A Packet Destination";
	rdfs:isDefinedBy	:;
.

:IP
	a					rdfs:Class;
	rdfs:subClassOf     :Packet;
	rdfs:label			"IP Packet";
	rdfs:comment 		"An IP Packet";
	rdfs:isDefinedBy	:;
.
:TCP
	a					rdfs:Class;
	rdfs:subClassOf     :IP;
	rdfs:label			"TCP Packet";
	rdfs:comment 		"A TCP Packet";
	rdfs:isDefinedBy	:;
.
:UDP
	a					rdfs:Class;
	rdfs:subClassOf     :IP;
	rdfs:label			"UDP Packet";
	rdfs:comment 		"A UDP Packet";
	rdfs:isDefinedBy	:;
.
:ICMP
	a					rdfs:Class;
	rdfs:subClassOf     :Packet;
	rdfs:label			"ICMP Packet";
	rdfs:comment 		"An ICMP Packet";
	rdfs:isDefinedBy	:;
.

:IGMP
	a					rdfs:Class;
	rdfs:subClassOf     :Packet;
	rdfs:label			"IGMP Packet";
	rdfs:comment 		"An IGMP Packet";
	rdfs:isDefinedBy	:;
.

:ARP
	a					rdfs:Class;
	rdfs:subClassOf     :Packet;
	rdfs:label			"ARP Packet";
	rdfs:comment 		"An ARP Packet";
	rdfs:isDefinedBy	:;
.

:endpoint
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"endpoint";
	rdfs:range  		:Host;
.

:src
	a					owl:ObjectProperty;
	rdfs:subPropertyOf  :endpoint;
	rdfs:isDefinedBy	:;
	rdfs:label 			"from";
	rdfs:domain			:Packet;
	rdfs:range  		:Source;
.

:srcPort
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"from port";
	rdfs:domain			:Packet;
	rdfs:range  		iana:Port;
.

:srcMAC
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"from MAC";
	rdfs:domain			:Packet;
	rdfs:range  		oui:Address;
.


:dst
	a					owl:ObjectProperty;
	rdfs:subPropertyOf  :endpoint;
	rdfs:isDefinedBy	:;
	rdfs:label 			"to";
	rdfs:domain			:Packet;
	rdfs:range  		:Destination;
.

:dstPort
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"to port";
	rdfs:domain			:Packet;
	rdfs:range  		iana:Port;
.

:dstMAC
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"to MAC";
	rdfs:domain			:Packet;
	rdfs:range  		oui:Address;
.

:session
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"session";
	rdfs:domain			:Packet;
	rdfs:range  		:Session;
.

:droppedCount
	a					owl:DatatypeProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"# dropped";
	rdfs:domain			:Packet;
	rdfs:range  		xsd:integer;
.

:receivedCount
	a					owl:DatatypeProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"# received";
	rdfs:domain			:Packet;
	rdfs:range  		xsd:integer;
.

:network
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"network #";
	rdfs:domain			:Packet;
	rdfs:range  		:Network;
.

:netmask
	a					owl:DatatypeProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"netmask";
	rdfs:domain			:Packet;
	rdfs:range  		xsd:string;
.

:linkType
	a					owl:ObjectProperty;
	rdfs:isDefinedBy	:;
	rdfs:label 			"Link Type";
	rdfs:domain			:Packet;
	rdfs:range  		:LinkType;
.
