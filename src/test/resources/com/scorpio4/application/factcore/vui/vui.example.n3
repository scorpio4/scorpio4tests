.

:vui04
    vui:contains [
        vui:asa         vui:VerticalLayout;
        vui:caption     "v4";
        vui:description "V4";
        vui:contains    [
            vui:asa         vui:Uploader;
            vui:caption     "v4 l1 b1";
            vui:description "v4 l1 b1";
        ];
        vui:contains    [
            vui:asa         vui:AceEditor;
            vui:caption     "v4 w1";
            vui:description "V4 W1";
            fact:model      :model04;
            do:perform [
                do:if       :onUpdate;
                do:then     :doUpdate;
                do:catch    :doUpdateFailed;
            ];
        ];
    ];
.
